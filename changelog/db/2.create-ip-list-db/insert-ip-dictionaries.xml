  <changeSet author="greatstep" id="create_table-ip_location_lite_db3">
    <createTable tableName="ip_location_lite_db3" schemaName="db_ip_list">
      <column name="id" type="bigint" autoIncrement="true">
        <constraints primaryKey="true"
          primaryKeyName="pk_ip_location_lite_db3_id" unique="true"/>
      </column>
      <column name="ip_in" type="bigint"/>
      <column name="ip_out" type="bigint"/>
      <column name="country_code" type="varchar(5)"/>
      <column name="country" type="varchar(100)"/>
      <column name="state" type="varchar(100)"/>
      <column name="city" type="varchar(100)"/>
    </createTable>
  </changeSet>

